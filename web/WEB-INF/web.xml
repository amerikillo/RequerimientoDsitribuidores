<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Loguear</servlet-name>
        <servlet-class>Login.Loguear</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Capturar</servlet-name>
        <servlet-class>Pedidos.Capturar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JQInvenCiclico</servlet-name>
        <servlet-class>Insumo.JQInvenCiclico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUpload</servlet-name>
        <servlet-class>in.co.sneh.model.FileUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>in.co.sneh.controller.FileUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Loguear</servlet-name>
        <url-pattern>/Loguear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Capturar</servlet-name>
        <url-pattern>/Capturar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JQInvenCiclico</servlet-name>
        <url-pattern>/JQInvenCiclico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUpload</servlet-name>
        <url-pattern>/FileUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/FileUploadServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            3600
        </session-timeout>
    </session-config>
</web-app>
